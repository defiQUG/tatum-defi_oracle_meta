{
  "name": "defi-oracle-meta",
  "version": "1.0.0",
  "description": "DeFi Oracle Meta - Secure Web3 Banking Portal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:security": "snyk test && npm audit",
    "test:integration": "jest --config jest.integration.config.js",
    "test:load": "artillery run tests/load/scenarios.yml",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install && pnpm audit",
    "audit": "pnpm audit",
    "clean": "rimraf dist node_modules pnpm-lock.yaml coverage .nyc_output",
    "clean:dist": "rimraf dist",
    "clean:modules": "rimraf node_modules",
    "reinstall": "pnpm clean && pnpm install",
    "build": "pnpm clean:dist && tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "build:prod": "cross-env NODE_ENV=production pnpm build",
    "docker:build": "docker build -t defi-oracle-meta .",
    "docker:run": "docker run -p 3000:3000 defi-oracle-meta",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "db:migrate": "mongoose-migrate up",
    "db:rollback": "mongoose-migrate down",
    "db:seed": "ts-node scripts/seed.ts",
    "db:reset": "pnpm db:rollback && pnpm db:migrate && pnpm db:seed",
    "docs": "typedoc && pnpm swagger",
    "swagger": "tsoa spec-and-routes",
    "precommit": "lint-staged && pnpm test",
    "security:audit": "pnpm audit && snyk test",
    "security:fix": "pnpm audit fix && snyk wizard",
    "security:compliance": "license-checker --production --summary",
    "analyze": "cross-env NODE_ENV=production webpack-bundle-analyzer dist/stats.json",
    "analyze:deps": "npx depcheck",
    "validate": "pnpm lint && pnpm test && pnpm security:audit",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "start:cluster": "cross-env NODE_ENV=production node dist/cluster.js",
    "monitor": "cross-env NODE_ENV=production node -r dd-trace/init dist/index.js",
    "monitor:apm": "elastic-apm-node start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/defiQUG/tatum-defi_oracle_meta.git"
  },
  "keywords": [
    "defi",
    "oracle",
    "blockchain",
    "web3",
    "tatum",
    "ethereum",
    "cryptocurrency",
    "banking",
    "fintech"
  ],
  "author": "DeFi Oracle Meta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defiQUG/tatum-defi_oracle_meta/issues"
  },
  "homepage": "https://github.com/defiQUG/tatum-defi_oracle_meta#readme",
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.3.1",
    "@tatumio/tatum": "^4.2.43",
    "ajv": "8.17.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.4",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dd-trace": "^5.25.0",
    "dotenv": "^16.4.5",
    "elastic-apm-node": "^4.4.1",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "express-csrf-double-submit-cookie": "^1.2.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "glob": "^11.0.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.4.1",
    "json2csv": "6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "mongoose": "^8.8.3",
    "pdfkit": "^0.15.1",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "rate-limiter-flexible": "^4.0.1",
    "superagent": "^10.1.1",
    "swagger-ui-express": "^5.0.0",
    "web3": "^4.15.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/pdfkit": "^0.13.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "artillery": "^2.0.3",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jest": "30.0.0-alpha.6",
    "license-checker": "^25.0.1",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "nyc": "^15.1.0",
    "prettier": "^3.4.1",
    "rimraf": "^5.0.5",
    "snyk": "^1.1283.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsoa": "^6.0.1",
    "typedoc": "^0.25.9",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "jest --findRelatedTests --passWithNoTests"
    ],
    "*.{ts,js,json,md,yml}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "glob": "^11.0.0",
      "test-exclude": "^7.0.0",
      "babel-plugin-istanbul": "^7.0.0"
    }
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": ".ts,.js",
    "ignore": ["src/**/*.test.ts", "src/**/*.spec.ts"],
    "exec": "ts-node ./src/index.ts"
  }
}
